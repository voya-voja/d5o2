{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "My C/C++: g++ build d5o_ta",
			"command": "/usr/bin/g++",
			"args": [
				"-fdiagnostics-color=always",
				"-std=c++17",
				"-g",
				"-D_WINDOWS",
				"${workspaceFolder}/dann5/source/*.cpp",
				"${workspaceFolder}/dann5/ocean/source/*.cpp",
				"${workspaceFolder}/d5o_ta/*.cpp",
				"-o",
				"${workspaceFolder}/d5o_ta/build/d5o_ta",
				"-I/usr/include/eigen3",
				"-I/usr/include/python3.10",
				"-I/home/voya/Envs/d5o/lib/python3.10/site-packages/pybind11/include",
				"-I${workspaceFolder}/dann5/include",
				"-I${workspaceFolder}/dann5/ocean/include",
				"-fpermissive",
				"-L/usr/lib/x86_64-linux-gnu/",
				"-lpython3.10"
			],
			"options": {
				"cwd": "${workspaceFolder}/d5o_ta"
			},
			"problemMatcher": {
				"base": "$gcc",
				"fileLocation": "absolute"
			},
			"group": "build",
			"detail": "compiler: /usr/bin/g++"
		},
		{
			"type": "cmake",
			"label": "CMake: configure",
			"command": "configure",
			"problemMatcher": [],
			"detail": "CMake template configure task",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"type": "cmake",
			"label": "CMake: build",
			"command": "build",
			"targets": [
				""
			],
			"group": "build",
			"problemMatcher": [],
			"detail": "CMake template build task"
		},
		{
			"type": "cppbuild",
			"label": "My C/C++: g++ compile active file",
			"command": "/usr/bin/g++",
			"args": [
				"-fdiagnostics-color=always",
				"-std=c++17",
				"-g",
				"-c",
				"${file}",
				"-o",
				"${workspaceFolder}/d5o_ta/build/${fileBasenameNoExtension}.o",
				"-I/usr/include/eigen3",
				"-I/usr/include/python3.10",
				"-I/home/voya/Envs/d5o/lib/python3.10/site-packages/pybind11/include",
				"-I${workspaceFolder}/dann5/include",
				"-I${workspaceFolder}/dann5/ocean/include",
				"-fpermissive"
			],
			"options": {
				"cwd": "${workspaceFolder}/d5o_ta"
			},
			"problemMatcher": {
				"base": "$gcc",
				"fileLocation": "absolute"
			},
			"group": "none",
			"detail": "compiler: /usr/bin/g++"
		},
	]
}
